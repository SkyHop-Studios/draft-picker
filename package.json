{
  "name": "app",
  "private": true,
  "scripts": {
    "generate:registry": "ts-node --project tsconfig.json .generators/service-registry/generator.ts",
    "generate:service": "ts-node --project tsconfig.json .generators/boilerplate/service.ts",
    "generate:collection": "ts-node --project tsconfig.json .generators/boilerplate/collection.ts",
    "generate:form": "ts-node --project tsconfig.json .generators/boilerplate/form.ts",
    "start": "meteor run",
    "test:meteor:ci": "cross-env meteor test --extra-packages \"insecure\" --once --driver-package meteortesting:mocha",
    "test:meteor:dev": "meteor test --extra-packages \"insecure\" --exclude-archs \"web.browser.legacy,web.cordova\" --driver-package meteortesting:mocha --port 3100",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.5",
    "@mantine/hooks": "^7.9.2",
    "@radix-ui/react-dialog": "^1.1.1",
